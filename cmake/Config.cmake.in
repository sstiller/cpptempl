get_filename_component(CPPTEMPL "${CMAKE_CURRENT_LIST_FILE}" DIRECTORY)
list(APPEND CMAKE_MODULE_PATH "${CPPTEMPL_CMAKE_DIR}")

if(NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
    include("${CPPTEMPL_CMAKE_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

set(@PROJECT_NAME@_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@)
